{
	"pageStrategy": "samsung_tizen",
	"modules": {
		"base": "antie/devices/browserdevice",
		"modifiers": [
		"antie/devices/anim/css3",
		"antie/devices/media/samsung_tizen",
		"antie/devices/mediaplayer/live/playable",
		"antie/devices/net/default",
		"antie/devices/data/nativejson",
		"antie/devices/storage/cookie",
		"antie/devices/logging/xhr",
		"antie/devices/exit/samsung_tizen",
		"antie/devices/exit/closewindow",
		"antie/devices/parentalguidance/appdefaultpghandler"
			]
	},
	"logging": {
		"level": "error",
		"strategy":"xhr"	
	},
	"streaming": {
		"video": {
			"mediaURIFormat": "%href%",
			"supported": [
			{
				"protocols": [
					"http"
					],
				"encodings": [
					"h264"
					],
				"transferFormat": [
					"hls",
				"plain"
					],
				"maximumBitRate": 3600,
				"maximumVideoLines": 1080
			}
			]
		},
		"audio": {
			"mediaURIFormat": "%href%",
			"supported": [
			{
				"protocols": [
					"http"
					],
				"encodings": [
					"aac"
					],
				"maximumBitRate": 192
			}
			]
		}
	},
	"keyCodeRef":"https://www.samsungdforum.com/TizenGuide/tizen3551/index.html",
	"input": {
		"map": {
			"13": "ENTER",
			"37": "LEFT",
			"38": "UP",
			"39": "RIGHT",
			"40": "DOWN",
			"10252": "PLAY_PAUSE",
			"415": "PLAY",
			"19": "PAUSE",
			"413": "STOP",
			"417": "FAST_FWD",
			"412": "REWIND",
			"10182": "BACK_SPACE",
			"10009": "BACK",
			"447": "VOLUME_UP",
			"448": "VOLUME_DOWN",
			"427": "NEXT",
			"428": "PREV",
			"449": "MUTE",
			"457": "HELP",
			"10073":  "SEARCH",
			"403": "RED",
			"404": "GREEN",
			"405": "YELLOW",
			"406": "BLUE",
			"48":"0",
			"49":"1",
			"50":"2",
			"51":"3",
			"52":"4",
			"53":"5",
			"54":"6",
			"55":"7",
			"56":"8",
			"57":"9"
		}
	},
	"accessibility": {
		"captions": {
			"supported": [
				"application/ttaf+xml"
				]
		}
	},
	"layouts": [
	{
		"width": 960,
		"height": 540,
		"module": "%application%/appui/layouts/540p",
		"classes": [
			"browserdevice540p"
			]
	},
	{
		"width": 1280,
		"height": 720,
		"module": "%application%/appui/layouts/720p",
		"classes": [
			"browserdevice720p"
			]
	},
	{
		"width": 1920,
		"height": 1080,
		"module": "%application%/appui/layouts/1080p",
		"classes": [
			"browserdevice1080p"
			]
	}
	],
		"networking": {
			"supportsJSONP": true
		}
}
